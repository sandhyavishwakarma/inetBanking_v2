<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>JBI Service Engine[inetBanking_v2]</name>
            <properties>
                <property name="ant.script">build.xml</property>
                <property name="user.properties.file">../build.properties</property>
                <property-file>${user.properties.file}</property-file>
                <property name="build.properties.file">build.properties</property>                
                <property-file>${build.properties.file}</property-file>
                <property-file>${netbeans.user}/build.properties</property-file>
            </properties>
            <folders>
                <source-folder>
                    <label>src/java</label>
                    <type>java</type>
                    <location>src/java</location>
                </source-folder>
                <source-folder>
                    <label>src/conf</label>
                    <type>java</type>
                    <location>src/conf</location>
                </source-folder>                
                <source-folder>
                    <label>test/java</label>
                    <type>java</type>
                    <location>test/java</location>
                </source-folder>                
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>package</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="run">
                    <script>${ant.script}</script>
                    <target>run</target>
                </action>
                <action name="test">
                    <script>${ant.script}</script>
                    <target>test</target>
                </action>                
                <action name="rebuild">
                    <script>${ant.script}</script>
                    <target>clean</target>
                    <target>package</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src/java</label>
                        <location>src/java</location>
                    </source-folder>
                    <source-folder style="tree">
                        <label>src/conf</label>
                        <location>src/conf</location>
                    </source-folder>                    
                    <source-folder style="packages">
                        <label>test/java</label>
                        <location>test/java</location>
                    </source-folder>  
                    <source-folder style="tree">
                        <label>test/testSA</label>
                        <location>test/testSA</location>
                    </source-folder>                  
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                    <source-file>
                        <location>${build.properties.file}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="clean"/>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="run"/> 
                    <separator/>
                    <ide-action name="test"/>
                    <action>
                        <script>${ant.script}</script>
                        <label>Test Component</label>
                        <target>integration-test</target>
                    </action>                                        
                    <separator/>
                    <action>
                        <script>${ant.script}</script>
                        <label>Install Component</label>
                        <target>install</target>
                    </action>
                    <action>
                        <script>${ant.script}</script>
                        <label>Uninstall Component</label>
                        <target>uninstall</target>
                    </action>
                    <action>
                        <script>${ant.script}</script>
                        <label>Show Component</label>
                        <target>show</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>${src.dir}</package-root>
                <classpath mode="compile">${javac.classpath}</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>${test.src.dir}</package-root>
                <classpath mode="compile">${javac.test.classpath}</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>            
        </java-data>
    </configuration>
</project>
